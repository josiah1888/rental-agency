{
  "name": "rental-agency",
  "version": "0.1.0",
  "description": "angular2 + typescript + firebase demo",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/josiah1888/rental-agency.git"
  },
  "scripts": {
    "postinstall": "typings install --ambient",
    "clean": "rimraf built/",
    "copy": "cp src/{index.html,system-config.js} built/",
    "copytemplates": "cp src/app/{*.html} built/app/",
    "build": "tsc && npm run copy && npm run copytemplates && npm run styles",
    "watch": "tsc --watch",
    "serve": "http-server -p 9090 -c-1",
    "styles": "node-sass --output-style compressed src/styles.scss > built/styles.css",
    "test": "karma start karma.conf.js"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.0",
    "@angular/compiler": "2.0.0-rc.0",
    "@angular/core": "2.0.0-rc.0",
    "@angular/platform-browser": "2.0.0-rc.0",
    "@angular/platform-browser-dynamic": "2.0.0-rc.0",
    "clang-format": "^1.0.35",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "http-server": "^0.8.5",
    "jasmine": "2.3.2",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "^0.0.4",
    "karma-jasmine": "^0.3.6",
    "node-sass": "^3.7.0",
    "rimraf": "^2.4.3",
    "typescript": "1.8.10",
    "typings": "^0.6.6"
  }
}
